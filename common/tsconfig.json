{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "target": "ESNext",
    "baseUrl": ".",
    "rootDir": ".",
    "strictFunctionTypes": true,
    "outDir": "ts-out",
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "ts-out"
  ]
}
