{
  "name": "ott-common",
  "version": "0.10.0",
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "module": "index.ts",
  "files": [
    "*"
  ],
  "exports": {
    ".": {
      "lean": "./ts-out/index.js",
      "default": "./index.ts"
    },
    "./*": {
      "lean": "./ts-out/*",
      "default": "./*"
    }
  },
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit && eslint --ext .js,.ts --fix .",
    "lint-ci": "tsc --noEmit && eslint .",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "dayjs": "^1.10.4"
  },
  "devDependencies": {
    "eslint-plugin-vitest": "0.3.22",
    "vitest": "^1.2.2"
  }
}
